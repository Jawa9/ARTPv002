<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.learn.neko.artp">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/vamp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/vamp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".ActivityDoNotTouch" />
        <activity android:name=".ActivityBad" />
        <activity android:name=".ActivityRight" />
        <activity android:name=".ActivitySomebodyKnow" />
        <activity android:name=".ActivityPP" />
        <activity android:name=".ActivitySvalit" />
        <activity android:name=".ActivityZb"></activity>
    </application>

</manifest>